<?xml version="1.0" encoding="UTF-8"?>
<testadapter>
  <codec encode="">
    <plugin id="com.testingtech.ttcn.XTestAdapter.builtin.tci.codecs">
      <parameter id="class" value="com.testingtech.ttcn.tci.codec.plugin.BitBaseCodecPluginProvider"/>
    </plugin>
  </codec>
  <codec encode="json">
    <plugin id="com.testingtech.ttworkbench.json.rt">
      <parameter id="class" value="com.testingtech.ttworkbench.json.rt.JSONCodecPlugin"/>
    </plugin>
    <parameter id="taParams">
      <parameter id="pretty-print">
        <value>
          <Values:boolean xmlns:Values="Values.xsd" type="boolean">
            <Values:value>true</Values:value>
          </Values:boolean>
        </value>
      </parameter>
    </parameter>
    <parameter id="default" value="true"/>
  </codec>
  <port>
    <plugin id="com.testingtech.ttworkbench.tt3rt.tcp.port.TcpPortPlugin">
      <parameter id="class" value="com.testingtech.ttworkbench.tt3rt.tcp.port.TcpPortPluginProvider"/>
    </plugin>
    <parameter id="taParams">
      <parameter id="TCP_MODE">
        <value>
          <Values:charstring name="" type="charstring">
            <Values:value>mixed</Values:value>
          </Values:charstring>
        </value>
      </parameter>
      <parameter id="TCP_LOCAL_HOST">
        <value>
          <Values:charstring name="" type="charstring">
            <Values:value>localhost</Values:value>
          </Values:charstring>
        </value>
      </parameter>
      <parameter id="TCP_LOCAL_PORT">
        <value>
          <Values:integer name="" type="integer">
            <Values:value>5000</Values:value>
          </Values:integer>
        </value>
      </parameter>
      <parameter id="TCP_REMOTE_HOST">
        <value>
          <Values:charstring name="" type="charstring">
            <Values:value>localhost</Values:value>
          </Values:charstring>
        </value>
      </parameter>
      <parameter id="TCP_REMOTE_PORT">
        <value>
          <Values:integer name="" type="integer">
            <Values:value>5000</Values:value>
          </Values:integer>
        </value>
      </parameter>
      <parameter id="TCP_SERVER_WAIT_TIME">
        <value>
          <Values:integer xmlns:Values="Values.xsd" type="integer">
            <Values:value>-1</Values:value>
          </Values:integer>
        </value>
      </parameter>
      <parameter id="portFilters">
        <parameter id="CAN_Filter"/>
      </parameter>
    </parameter>
    <parameter id="portFilters">
      <parameter id="portFilter">
        <parameter id="pluginId" value="CAN_Filter"/>
        <parameter id="class" value="CANPortFilter.CANPortFilterProvider"/>
      </parameter>
    </parameter>
    <parameter id="portType" value="Car2X_ConfigSystem.FilterPortType"/>
    <parameter id="default" value="true"/>
  </port>
</testadapter>
